  "firecrawl": {
    "api_key_env": "FIRECRAWL_API_KEY",
    "base_url": "https://api.firecrawl.dev/v1",
    "rate_limit_per_minute": 10,
    "max_retries": 3,
    "timeout_seconds": 120
  },
  "supabase": {
    "url_env": "SUPABASE_URL",
    "key_env": "SUPABASE_SERVICE_KEY",
    "table": "zoning_districts"
  },
  "parsing": {
    "district_patterns": [
      "ยง\s*(\d+-\d+)\.",
      "Sec\.?\s*(\d+-\d+)",
      "Section\s*(\d+-\d+)"
    ],
    "category_keywords": {
      "Residential": ["residential", "single-family", "multi-family", "duplex", "dwelling"],
      "Commercial": ["commercial", "business", "retail", "office"],
      "Industrial": ["industrial", "manufacturing", "warehouse"],
      "Mixed-Use": ["mixed-use", "mixed use", "planned unit"],
      "Agricultural": ["agricultural", "rural", "farming"],
      "Institutional": ["institutional", "public", "government"]
    }
  }
